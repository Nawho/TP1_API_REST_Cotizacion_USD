{
  "name": "tp1-api-rest-cotizacion-usd",
  "version": "1.0.0",
  "description": "Una API para obtener la cotiazación acutal de los 12 tipos de dólares en Argentina, así como su valor histórico y un cron job que actualizará la base de datos cada 1h.",
  "main": "index.js",
  "scripts": {
    "api": "npx nodemon ./src/api/index.ts",
    "cron": "npx nodemon ./src/cron-updater/index.ts",
    "build": "npx tsc"
  },
  "keywords": [
    "express",
    "api",
    "economy",
    "finances",
    "dolar"
  ],
  "author": "M4litt & Nawho",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.3.3",
    "@typegoose/typegoose": "^11.0.0",
    "puppeteer": "^19.8.2",
    "dotenv": "^16.0.3",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "@types/puppeteer": "^7.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}